!!!
html
	head
		title Cloud Dial
		link(rel='stylesheet', href='/stylesheets/style.css')
		script(src='/javascripts/libs/jquery-1.7.1.js')
		script(src='/javascripts/libs/underscore-1.3.1.js')
		script(src='/javascripts/libs/backbone-0.5.3.js')
		script(src='/javascripts/libs/json2.js')
		script(src='/socket.io/socket.io.js')
		script(src='/socket.io/socket.io-sessions.js')
	body!= body
		-if(everyauth.loggedIn)
			h3
				a(href='/logout') Logout
		script(type='text/template', id='bookmark-template')
			<a href="<%= address %>"><img src="<%= imgAddress %>"/></a>
		script(type='text/template', id='group-template')
			<div><%= name %></div>
		script(type='text/template', id='tag-template')
			<div><%= tag %></div>
		script(src='/javascripts/clouddial.js')